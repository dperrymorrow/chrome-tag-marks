{
  "manifest_version": 2,

  "name": "tag-marks",
  "description": "Tag and search your Chrome bookmarks",
  "version": "1.0",

  "permissions": [
    "bookmarks"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "sandbox": {
     "pages": ["index.html"]
  }
}